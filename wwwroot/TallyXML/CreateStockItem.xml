					<SVCURRENTCOMPANY>{Cname}</SVCURRENTCOMPANY>
				</STATICVARIABLES>
			</REQUESTDESC>
			<REQUESTDATA>
				<TALLYMESSAGE xmlns:UDF="TallyUDF">
