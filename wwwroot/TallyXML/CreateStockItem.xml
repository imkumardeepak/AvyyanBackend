							<EXPIRYPERIOD/>
						</BATCHALLOCATIONS.LIST>
						<GSTAPPLICABLE>Applicable</GSTAPPLICABLE>
						<HSNDETAILS.LIST>
							<APPLICABLEFROM>20240401</APPLICABLEFROM>
							<HSNCODE>{BIND_HSN}</HSNCODE>
							<HSN/>
