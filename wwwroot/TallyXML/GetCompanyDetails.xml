<ENVELOPE>
	<HEADER>
		<VERSION>1</VERSION>
		<TALLYREQUEST>EXPORT</TALLYREQUEST>
		<TYPE>COLLECTION</TYPE>
		<ID>CompanyLedgerDetails</ID>
	</HEADER>
	<BODY>
		<DESC>
			<STATICVARIABLES>
				<SVEXPORTFORMAT>$$SysName:XML</SVEXPORTFORMAT>
				<SVCURRENTCOMPANY>{Cname}</SVCURRENTCOMPANY>
			</STATICVARIABLES>
			<TDL>
				<TDLMESSAGE>
					<COLLECTION NAME="CompanyLedgerDetails" ISINITIALIZE="Yes">
						<TYPE>Ledger</TYPE>
						<NATIVEMETHOD>Name</NATIVEMETHOD>
						<NATIVEMETHOD>Address</NATIVEMETHOD>
						<NATIVEMETHOD>Email</NATIVEMETHOD>
						<NATIVEMETHOD>Phone</NATIVEMETHOD>
						<NATIVEMETHOD>Contact</NATIVEMETHOD>
						<NATIVEMETHOD>PinCode</NATIVEMETHOD>
						<NATIVEMETHOD>State</NATIVEMETHOD>
						<NATIVEMETHOD>Country</NATIVEMETHOD>
						<NATIVEMETHOD>GSTIN</NATIVEMETHOD>
						<NATIVEMETHOD>PAN</NATIVEMETHOD>
						<NATIVEMETHOD>City</NATIVEMETHOD>
						<FILTERS>IsCompanyFilter</FILTERS>
					</COLLECTION>
					<SYSTEM TYPE="Formulae" NAME="IsCompanyFilter">$$IsCompany:Ledger</SYSTEM>
				</TDLMESSAGE>
			</TDL>
		</DESC>
	</BODY>
</ENVELOPE>